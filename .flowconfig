[ignore]
.*/node_modules/.*
.*/coverage/.*
.*/test/.*
.*/scripts/.*

[include]

[libs]

[lints]
all=error
unclear-type=off

[options]
all=true
module.name_mapper='^\$/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.ignore_non_literal_requires=true

[strict]
nonstrict-import
unsafe-getters-setters
untyped-import
untyped-type-import
